{
  "version": 2,
  "env": {
    "API_PORT": "3000",
    "POST_PORT": "3002",

    "NODE_ENV": "production",
    "MYSQL_HOST": "localhost",
    "MYSQL_USER": "root",
    "MYSQL_PASSWORD": "lauti4321",
    "MYSQL_DB": "db_node_usuarios",

    "MYSQL_SRV_HOST": "localhost",
    "MYSQL_SRV_PORT": "3001",

    "JWT_SECRET": "notasecret"
  },
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "post/index-post.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/auth(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/api/user(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/api/post(.*)",
      "dest": "/post/index-post.js"
    }
  ]
}